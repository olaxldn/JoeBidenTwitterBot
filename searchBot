ACCESS_KEY = '1318216558519484416-NVkKJvTyKZ8Qdq6y650O3lXDbPXRpu'
ACCESS_SECRET = 'B86OmT0wuLf94RviNQace8tEP0cGGIfJya0wjmIUEuUxU'

auth = tweepy.OAuthHandler(CONSUMER_KEY, CONSUMER_SECRET)
auth.set_access_token(ACCESS_KEY, ACCESS_SECRET)
api = tweepy.API(auth)

hashtag = "#1910hours"
tweetNumber = 10

tweets = tweepy.Cursor(api.search, hashtag).items(tweetNumber)

def	searchBot():
 for tweet in tweets:
  try:
      tweet.retweet()
      print("Retweet done!")
      time.sleep(2)
  except tweepy.TweepError as e:
       print(e.reason)
       time.sleep(2)

 searchBot()	  